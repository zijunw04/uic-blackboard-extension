{
  "manifest_version": 2,
  "name": "Check ID Extension",
  "version": "1.0",
  "description": "Check if a website has a certain ID.",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": {
      "48": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
